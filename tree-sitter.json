{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "strings",
      "camelcase": "Strings",
      "title": "Strings",
      "scope": "source.strings",
      "file-types": [
        "strings"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^strings$",
      "class-name": "TreeSitterStrings"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "xcode strings",
    "authors": [
      {
        "name": "Carson",
        "email": "jackparadise520a@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/CarsonSlovoka/tree-sitter-strings"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
